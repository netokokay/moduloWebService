const express = require('express');
const app = express();

app.listen(3000, () => {
    console.log('Server started on port 3000');
});



---------------------------------


app.get('/route1', (req, res) => {
    res.json({ message: 'Rota 1' });
});

app.post('/route2', (req, res) => {
    res.json({ message: 'Rota 2' });
});
